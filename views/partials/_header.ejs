<h1>
    TinyURL Maker!!!!
</h1>
<body>
  <table>
    <% if (username) { %>
    <table>
      <td>
        user: <%= username %>
      </td>
      <td>
        <form method="POST" action="/logout" style="margin:10px">
          <input type='submit' value='logout'>
        </form>
      </td>
    </table>
    <% } else { %>
    <tr>
      <form method="POST" action="/login" style="margin:10px">
        <input id=login type='text' name='username' size= '40'>
        <input type='submit' value='login'>
    </tr> <% } %>
  </form>
</body>